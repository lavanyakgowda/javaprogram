package javaproject;

public class Ternery {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 10, b = 100, c = 300;

        int largest = (a > b) ? (a > c ? a : c) 
                              : (b > c ? b : c);

        System.out.println("The largest number is: " + largest);
    }
}
	


